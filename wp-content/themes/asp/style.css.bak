ol.bigbullets {
    margin: 0;
    padding: 0rem 0 0 0.75rem;
    list-style-type: none;
    counter-reset: listStyle;
}
ol.bigbullets li {
    counter-increment: step-counter;
    counter-increment: listStyle;
    margin-bottom: 1.5rem;
    min-height: 1.6rem;
}
.bigbullets.reset14{counter-reset: listStyle 14;}
.bigbullets.reset13{counter-reset: listStyle 13;}

ol.bigbullets li.yellow:before{
	background-color: #fef309;
}
ol.bigbullets li.grey:before{
	background-color: #ccc;
}
.bigbullets li:before {
    content: counter(step-counter);
    content: counter(listStyle, decimal);
    font-size: 1.25rem;
    border: 1px solid;
    font-weight: 600;
    border-radius: 50%;
    display: block;
    text-align: center;
    align-items: center;
    float: left;
    height: 2.1rem;
    width: 2.1rem;
    left: -0.75rem;
    position: relative;
    top: -0.3rem;
}
.bigbullets.latin li:before {
	content: counter(listStyle, upper-alpha);
}
