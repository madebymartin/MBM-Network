a.featuredlink{
    display: inline-flex;
    height: 25vh;
    align-items: center;
    text-align: center;
    filter: grayscale(0.8);
    color: #fff;
    font-size: 2rem;
    transition: all 0.3s cubic-bezier(0, 0, 0.2, 1);
    position: relative;
    padding: 1rem;
    float: left;
    overflow: hidden;
    font-weight: 300;
    text-transform: uppercase;
    line-height: 1;
    text-decoration: none;
    margin-bottom: 0.8rem;
}
a.featuredlink .background{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    background-size: cover;
    background-position: center;
    z-index: -1;
    transform: scale(1.2) rotate(0);
    transition: all 0.3s ease;
}
a.featuredlink:hover .background, a.featuredlink:active .background, a.featuredlink:focus .background{
	transform: scale(2) rotate(6deg);
}



a.featuredlink span{
	width: 100%;
	text-align: center;
}
a.featuredlink:hover, a.featuredlink:active, a.featuredlink:focus{
